      int valor1;
      scanf("%d", &valor1);
      printf("Digite o segundo valor: ");
      int valor2;
      scanf("%d", &valor2);
      printf("Digite o terceiro valor: ");
      int valor3;
      scanf("%d", &valor3);

      printf("A soma dos valores digitados Ã© igual a %d", valor1 + valor2 + valor3);
}
